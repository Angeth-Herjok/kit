{
  "name": "@openfn/runtime-manager",
  "version": "0.0.5",
  "description": "An example runtime manager service.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "pnpm ava",
    "build": "rimraf dist/ .rollup.cache && rollup -c",
    "watch": "pnpm rollup -cw --no-watch.clearScreen",
    "serve": "nodemon -e ts --exec 'tsm --experimental-vm-modules --no-warnings src/server/index.ts'"
  },
  "author": "Joe Clark <jclark@openfn.org>",
  "license": "ISC",
  "dependencies": {
    "@openfn/compiler": "workspace:^0.0.7",
    "@openfn/language-common": "2.0.0-rc3",
    "@openfn/runtime": "workspace:^0.0.4",
    "@types/koa": "^2.13.5",
    "@types/workerpool": "^6.1.0",
    "koa": "^2.13.4",
    "tsm": "^2.2.2",
    "workerpool": "^6.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.31",
    "ava": "^4.2.0",
    "nodemon": "^2.0.19",
    "rollup": "^2.72.1",
    "rollup-plugin-dts": "^4.2.1",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
