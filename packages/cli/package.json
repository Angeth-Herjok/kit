    "rollup": "^2.72.1",
    "rollup-plugin-dts": "^4.2.1",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@openfn/compiler": "workspace:^0.0.1",
    "@openfn/runtime": "workspace:^0.0.1",
