{
  "name": "@openfn/cli",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
